{
    "name" : "JavaEE API Search",
    "description" : "Adds support to the omnibox to search the JavaEE API.",
    "manifest_version" : 2, 
    "background" : {
         "scripts" : [ 
         	"index.js",
         	"String.js",
         	"background.js" 
         ]
    },
    "icons" : { 
    	"128" : "icons/icon128.png", 
    	"32" : "icons/icon32.png", 
    	"16" : "icons/icon16.png" 
    },
    "omnibox" : { 
    	"keyword" : "jee" 
    },
    "permissions" : [
        "tabs",
        "http://docs.oracle.com/",
        "https://docs.oracle.com/"
    ],
    "version":"3.0.0"
}
