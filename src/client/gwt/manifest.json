{
    "name" : "Web Toolkit™ API Search",
    "description" : "Adds support to the omnibox to search within the Google Web Toolkit™ API.",
    "manifest_version" : 2, 
    "background" : {
         "scripts" : [ "background.js" ]
    },
    "icons" : { "128" : "icon128.png", "32" : "icon32.png", "16" : "icon16.png" },
    "omnibox":{ "keyword" : "gwt" },
    "permissions":[
        "tabs",
        "*://google-web-toolkit.googlecode.com/"
    ],
    "version" : "3.2"
}
