{
	"name" : "Qt API Search",
	"description" : "Adds support to the omnibox to search the Qt API.",
	"manifest_version" : 2, 
    "background" : {
         "scripts" : [ "background.js" ]
    },
	"icons" : {
		"128" : "icon128.png",
		"32" : "icon32.png",
		"16" : "icon16.png"
	},
	"omnibox" : {
		"keyword" : "qt"
	},
	"permissions" : [ 
		"tabs", 
		"*://doc.qt.nokia.com/" 
	],
	"version" : "2.0"
}
